# Basic Configuration
# Copy this file to .env.local and adjust values as needed

# Service Worker
NEXT_SERVICE_WORKER_ENABLED=true
NEXT_OFFLINE_SUPPORT=true

# Image Optimization
NEXT_IMAGE_OPTIMIZATION=true
NEXT_IMAGE_FORMATS=webp,avif
NEXT_IMAGE_QUALITY=75

# Static Generation
NEXT_STATIC_GENERATION=true
NEXT_ISR_ENABLED=true
NEXT_ISR_REVALIDATE=3600

# Development vs Production
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
